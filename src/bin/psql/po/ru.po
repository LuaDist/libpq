#: help.c:169
#, c-format
msgid "General\n"
msgstr "Общие\n"

#: help.c:170
#, c-format
msgid ""
"  \\copyright             show PostgreSQL usage and distribution terms\n"
msgstr ""
"  \\copyright             условия использования и распространения "
"PostgreSQL\n"

#: help.c:171
#, c-format
msgid ""
"  \\g [FILE] or ;         execute query (and send results to file or |pipe)\n"
msgstr ""
