msgstr ""
"  -p, --port=ПОРТ          порт сервера баз данных (по умолчанию: \"%s\")\n"

#: help.c:144
#, c-format
msgid "  -U, --username=USERNAME  database user name (default: \"%s\")\n"
msgstr "  -U, --username=ИМЯ       имя пользователя (по умолчанию: \"%s\")\n"

