msgstr "  \\dg[+]  [МАСКА]        список ролей\n"

#: help.c:217
#, c-format
msgid "  \\di[S+] [PATTERN]      list indexes\n"
msgstr "  \\di[S+] [МАСКА]        список индексов\n"

