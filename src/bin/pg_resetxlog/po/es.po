msgid "Try \"%s --help\" for more information.\n"
msgstr "Prueba con «%s --help» para más información\n"

#: pg_resetxlog.c:141
#, c-format
msgid "%s: transaction ID epoch (-e) must not be -1\n"
msgstr "%s: el «epoch» de ID de transacción (-e) no debe ser -1\n"

